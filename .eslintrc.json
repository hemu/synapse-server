{
    "extends": "airbnb",
    "rules": {
      "no-trailing-spaces": ["error", { "skipBlankLines": false }],
      "no-underscore-dangle": "off",
      "no-unused-vars": 1,
      "new-cap": 0,
      "no-console": "off",
      "import/no-extraneous-dependencies": ["error", {"devDependencies": true}]
    },
    "parserOptions":{
      "ecmaFeatures": {
        "experimentalObjectRestSpread": true
      }
    },
    "plugins": [
        "react",
        "jsx-a11y",
        "import"
    ]
}
