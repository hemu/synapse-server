    "test-single-controller": "clear && blue-tape -r babel-register src/test/controller.test.js | tap-summary",
    "test-single-graph": "clear && blue-tape -r babel-register src/test/graph_db.test.js",
    "test-single-smodel": "clear && blue-tape -r babel-register src/test/controller_student_model.test.js",
    "test-single-srs": "clear && blue-tape -r babel-register src/test/spaced_repetition.test.js",
    "docs": "watch 'rm -rf docs && docco src/**/**.js && rm -rf docs/*.test.html' src",
    "start": "nodemon register.js",
    "start-clean": "npm run clean-logs && npm run start",
    "clean-logs": "rm -rf logs/*.log"
  },
